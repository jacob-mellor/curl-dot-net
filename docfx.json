{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/CurlDotNet/CurlDotNet.csproj"],
          "src": "."
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "docs/**.md",
          "docs/**/toc.yml",
          "*.md",
          "toc.yml"
        ],
        "exclude": [
          "**/README.md",
          "**/node_modules/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "docs/images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "markdigExtensions": [
        "definitionlists",
        "footnotes",
        "autoidentifiers",
        "tasklists",
        "pipetables",
        "listextras",
        "emphasisextras",
        "diagrams"
      ]
    },
    "globalMetadata": {
      "_appTitle": "CurlDotNet",
      "_appName": "CurlDotNet Documentation",
      "_appFooter": "Copyright Â© 2024 Jacob Mellor - Sponsored by IronSoftware",
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/favicon.ico",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_gitContribute": {
        "repo": "https://github.com/jacob-mellor/curl-dot-net",
        "branch": "master"
      },
      "author": "Jacob Mellor",
      "authorUrl": "https://github.com/jacob-mellor",
      "sponsor": "IronSoftware",
      "sponsorUrl": "https://ironsoftware.com"
    },
    "fileMetadata": {
      "priority": {
        "api/**/Curl.yml": 1000,
        "api/**/CurlRequestBuilder.yml": 900
      }
    },
    "sitemap": {
      "baseUrl": "https://jacob-mellor.github.io/curl-dot-net",
      "priority": 0.5,
      "changefreq": "weekly"
    },
    "xrefService": ["https://xref.docs.microsoft.com/query?uid={uid}"],
    "xref": [],
    "noLangKeyword": false
  }
}